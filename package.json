{
  "name": "meteor-typescript-bootstrap",
  "version": "0.2.0",
  "description": "A project which contains the bare bones of a Meteor app based on typescript.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:webpack && npm run build:public",
    "build:prod": "npm run clean && npm run build:webpack:prod && npm run build:public && npm run meteor:build",
    "build:public": "mkdirp build/public && cpx public/** build/public",
    "build:webpack": "webpack --config ./config/webpack.dev.config.js",
    "build:webpack:prod": "webpack --config ./config/webpack.prod.config.js",
    "clean": "rimraf build/server build/client build/lib build/public && mkdirp build/public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "meteor": "cd build && meteor run",
    "meteor:build": "cd build && meteor build ../dist",
    "meteor:mongo": "cd build && meteor mongo",
    "meteor:debug": "cd build && meteor debug",
    "meteor:deploy": "cd build && meteor deploy meteor-typescript-bootstrap.meteor.com",
    "setup": "cd build && meteor create tmp && cpx \"tmp/.meteor/**\" \".meteor\" && meteor remove insecure autopublish mobile-experience jquery ecmascript && meteor add accounts-ui accounts-google",
    "webpack": "npm run build:webpack",
    "presetup": "rimraf build && mkdirp build",
    "postinstall": "tsd reinstall -so --config config/tsd.json",
    "postsetup": "rimraf build/tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshir/meteor-typescript-bootstrap.git"
  },
  "keywords": [
    "meteor",
    "typescript"
  ],
  "author": "Paul Shirley",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/paulshir/meteor-typescript-bootstrap/issues"
  },
  "homepage": "https://github.com/paulshir/meteor-typescript-bootstrap#readme",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "cpx": "^1.2.1",
    "css-loader": "^0.19.0",
    "cssnext-loader": "^1.0.1",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.4",
    "ts-loader": "^0.5.6",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "normalize-css": "^2.3.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
