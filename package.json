{
  "name": "meteor-typescript-bootstrap",
  "version": "0.2.0",
  "description": "A project which contains the bare bones of a Meteor app based on typescript.",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpack && npm run build:html && npm run build:public",
    "build:html": "mkdirp build/client && cp -r html/**.html build/client",
    "build:public": "mkdirp build/public && cp -r public/** build/public",
    "clean": "rimraf build/server build/client build/lib build/public build/shared build/public && mkdirp build/public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "meteor": "cd build && meteor run",
    "meteor:mongo": "cd build && meteor mongo",
    "meteor:debug": "cd build && meteor debug",
    "meteor:deploy": "cd build && meteor deploy meteor-typescript-bootstrap.meteor.com",
    "setup": "cd build && meteor create tmp && cp -r tmp/.meteor . && meteor remove insecure autopublish && meteor add accounts-ui accounts-google react",
    "webpack": "webpack --config ./webpack/dev.config.js",
    "prebuild": "npm run clean",
    "presetup": "rimraf build && mkdirp build",
    "postsetup": "rimraf build/tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshir/meteor-typescript-bootstrap.git"
  },
  "keywords": [
    "meteor",
    "typescript"
  ],
  "author": "Paul Shirley",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/paulshir/meteor-typescript-bootstrap/issues"
  },
  "homepage": "https://github.com/paulshir/meteor-typescript-bootstrap#readme",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3",
    "ts-loader": "^0.5.5",
    "tsd": "^0.6.4",
    "typescript": "^1.6.2",
    "webpack": "^1.12.2"
  }
}
